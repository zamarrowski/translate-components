{
  "name": "translate-components",
  "version": "1.3.7",
  "description": "🌎🌍🌏 translate-components provides a super simple translation for React.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel ./src --out-dir ./lib --source-maps --presets es2015,react,stage-2 --plugins babel-plugin-add-module-exports",
    "start": "webpack --watch",
    "build": "webpack",
    "test": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zamarrowski/translate-components.git"
  },
  "keywords": [
    "react",
    "translate",
    "react-translate",
    "multilingual",
    "multilanguage",
    "translate-components"
  ],
  "author": "@zamarrowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zamarrowski/translate-components/issues"
  },
  "homepage": "https://github.com/zamarrowski/translate-components#readme",
  "dependencies": {
    "glob": "7.1.1",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2"
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel": "6.23.0",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "webpack": "2.2.1"
  }
